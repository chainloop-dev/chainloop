{
  "$id": "controlplane.v1.CASBackendServiceUpdateRequest.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "description": "Update a CAS backend is limited to\n - description\n - set is as default\n - rotate credentials\n - max blob size",
  "patternProperties": {
    "^(maxBytes)$": {
      "anyOf": [
        {
          "exclusiveMaximum": 9223372036854776000,
          "minimum": -9223372036854775808,
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "description": "Maximum size in bytes for each blob stored in this backend."
    }
  },
  "properties": {
    "credentials": {
      "$ref": "google.protobuf.Struct.schema.json",
      "description": "Credentials, useful for rotation"
    },
    "default": {
      "description": "Set as default in your organization",
      "type": "boolean"
    },
    "description": {
      "description": "Description",
      "type": "string"
    },
    "max_bytes": {
      "anyOf": [
        {
          "exclusiveMaximum": 9223372036854776000,
          "minimum": -9223372036854775808,
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "description": "Maximum size in bytes for each blob stored in this backend."
    },
    "name": {
      "type": "string"
    }
  },
  "title": "CAS Backend Service Update Request",
  "type": "object"
}
